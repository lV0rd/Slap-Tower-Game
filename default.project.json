{
  "name": "Slap Tower Game",
  "tree": {
    "$className": "DataModel",
    "ReplicatedStorage": {
      "Shared": {
        "$path": "src/shared"
      },
      "Source": {
        "$path": "src/client/Source"
      },
      "Packages": {
        "$path": "Packages"
      },
      "$ignoreUnknownInstances": true
    },
    "ServerStorage": {
      "Source": {
        "$path": "src/server/Source"
      },
      "$ignoreUnknownInstances": true
    },
    "ServerScriptService": {
      "Server": {
        "$path": "src/server/Loader.server.luau"
      },
      "Scripts": {
        "$path": "src/server/Scripts",
        "$ignoreUnknownInstances": true
      },
      "$ignoreUnknownInstances": true
    },
    "StarterPlayer": {
      "StarterPlayerScripts": {
        "Client": {
          "$path": "src/client/Loader.client.luau"
        },
        "Scripts": {
          "$path": "src/client/Scripts",
          "$ignoreUnknownInstances": true
        },
        "$ignoreUnknownInstances": true
      },
      "StarterCharacterScripts": {
        "Scripts": {
          "$path": "src/client/CharacterScripts",
          "$ignoreUnknownInstances": true
        },
        "$ignoreUnknownInstances": true
      }
    }
  }
}